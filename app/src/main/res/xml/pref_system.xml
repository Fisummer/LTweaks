            android:title="@string/pref_nav_bar_input_cursor_control"
            android:summary="@string/pref_nav_bar_input_cursor_control_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_native_clipboard">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_native_clipboard_fix_chrome"
            android:title="@string/pref_native_clipboard_fix_chrome"
            android:summary="@string/pref_native_clipboard_fix_chrome_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_trust_agent">
        <Preference
            android:key="@string/key_trust_agent_wifi_aps"
            android:title="@string/pref_trust_agent_wifi"
            android:summary="@string/pref_trust_agent_wifi_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_solid_explorer">
        <Preference
            android:key="@string/key_solid_explorer_url_replacers"
            android:title="@string/pref_solid_explorer_replace_url"
            android:summary="@string/pref_solid_explorer_replace_url_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_app_info">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_app_info_open_app_data_folder"
            android:title="@string/pref_app_info_open_app_data_folder"
            android:summary="@string/pref_app_info_open_app_data_folder_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_app_info_go_market"
            android:title="@string/pref_app_info_go_market"
            android:summary="@string/pref_app_info_go_market_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_app_info_allow_disable"
            android:title="@string/pref_app_info_allow_disable"
            android:summary="@string/pref_app_info_allow_disable_summary" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_app_info_app_ops"
            android:title="@string/pref_app_info_app_ops"
            android:summary="@string/pref_app_info_app_ops_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_lineage_os">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_lineage_os_live_display_time"
            android:title="@string/pref_lineage_os_live_display_time"
            android:summary="@string/pref_lineage_os_live_display_time_summary" />
        <li.lingfeng.ltweaks.fragments.base.TimePickerPreference
            android:defaultValue="08:00"
            android:key="@string/key_lineage_os_live_display_time_sunrise"
            android:title="@string/pref_lineage_os_live_display_time_sunrise" />
        <li.lingfeng.ltweaks.fragments.base.TimePickerPreference
            android:defaultValue="19:00"
            android:key="@string/key_lineage_os_live_display_time_sunset"
            android:title="@string/pref_lineage_os_live_display_time_sunset" />
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_lineage_os_gallery_remove_bottom_bar"
            android:title="@string/pref_lineage_os_gallery_remove_bottom_bar"
            android:summary="@string/pref_lineage_os_gallery_remove_bottom_bar_summary" />
