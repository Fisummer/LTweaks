    <PreferenceCategory
        android:title="@string/pref_category_keys">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_keys_long_press_back_to_kill"
            android:title="@string/pref_keys_long_press_back_to_kill"
            android:summary="@string/pref_keys_long_press_back_to_kill_summary" />
    </PreferenceCategory>

    <PreferenceCategory
        android:title="@string/pref_category_recent_task">
        <SwitchPreference
            android:defaultValue="false"
            android:key="@string/key_recent_task_swipe_to_kill"
            android:title="@string/pref_recent_task_swipe_to_kill"
            android:summary="@string/pref_recent_task_swipe_to_kill_summary" />
    </PreferenceCategory>
</PreferenceScreen>
